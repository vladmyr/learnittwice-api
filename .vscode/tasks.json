{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "0.1.0",
  "command": "npm",
  "isShellCommand": true,
  "suppressTaskName": true,
  "showOutput": "silent",
  "tasks": [{
    "taskName": "build:tsc",
    "problemMatcher": "$tsc",
    "isBuildCommand": true,
    "args": ["run", "build:tsc"]
  }, {
    "taskName": "watch:tsc",
    "problemMatcher": "$tsc",
    "isBuildCommand": true,
    "args": ["run", "watch:tsc"]
  }]
}