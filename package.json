{
  "name": "learnittwice-api",
  "version": "0.0.1",
  "description": "API webserver for LearnItTwice project",
  "main": "src/index.ts",
  "dependencies": {
    "@types/bluebird": "^3.0.35",
    "@types/db-migrate-base": "0.0.3",
    "@types/db-migrate-pg": "0.0.3",
    "@types/mongoose": "^4.5.41",
    "@types/node": "^6.0.46",
    "@types/pg": "^6.1.34",
    "@types/pg-pool": "0.0.2",
    "@types/pg-promise": "0.0.28",
    "@types/pg-types": "^1.11.1",
    "@types/underscore": "^1.7.33",
    "bluebird": "^3.4.6",
    "mongoose": "^4.6.7",
    "mongoose-deep-populate": "^3.0.0",
    "mongoose-populate-extended": "0.0.4",
    "pg": "^6.1.0",
    "pg-promise": "^5.5.0",
    "systemjs": "^0.19.41",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.35",
    "chai": "^3.5.0",
    "cli": "^1.0.1",
    "config": "^1.24.0",
    "cross-env": "^3.1.3",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.1.11",
    "mocha": "^3.2.0",
    "typescript": "^2.1.4"
  },
  "scripts": {
    "postinstall": "node -e \"var s='../src',d='node_modules/src',fs=require('fs');fs.exists(d,function(e){e||fs.symlinkSync(s,d,'dir')});\"",
    "build:tsc": "./node_modules/.bin/tsc",
    "watch:tsc": "./node_modules/.bin/tsc -w",
    "start:dev": "node ./bin/development.js",
    "start:test": "node ./bin/development_test.js",
    "db-migrate:dev": "./node_modules/.bin/db-migrate --env development --migrations-dir ./src/App/Persistence/Repositories/Postgres/Migrations --table __Migrations --",
    "db-migrate:test": "./node_modules/.bin/db-migrate --env development_test --migrations-dir ./src/App/Persistence/Repositories/Postgres/Migrations --table __Migrations --"
  },
  "author": "Volodymyr Khytskyi",
  "license": "MIT"
}
